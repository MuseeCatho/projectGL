<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="mapping.Proposition" table="proposition">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="date" type="date">
            <column name="date" not-null="false"/>
        </property>
        <property name="etat" type="string">
            <column name="etat" length="45" not-null="false"/>
        </property>
        <property name="type" type="string">
            <column name="type" length="45" not-null="false" />
        </property>
		<property name="id_audio" type="java.lang.Integer">
            <column name="id_audio" />
        </property>
        <property name="id_photos" type="java.lang.Integer">
            <column name="id_photos"/>
        </property>
        <property name="id_videos" type="java.lang.Integer">
            <column name="id_videos" />
        </property>
        <property name="id_enrichments" type="java.lang.Integer">
            <column name="id_enrichments" />
        </property>
    </class>
</hibernate-mapping>
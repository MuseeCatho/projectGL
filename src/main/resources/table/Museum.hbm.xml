<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="mapping.Museum" table="museum">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="address" type="string">
            <column name="address" length="100" not-null="false"/>
        </property>
        <property name="phone" type="string">
            <column name="phone" length="30" not-null="false"/>
        </property>
        <property name="mail" type="string">
            <column name="mail" length="100" not-null="false" />
        </property>
        <property name="access" type="string">
            <column name="access" length="200" not-null="false" />
        </property>
        <property name="schedule" type="string">
            <column name="schedule" length="300" not-null="false"/>
        </property>
        <property name="presentation_e" type="string">
            <column name="presentation_e" length="4000" not-null="false"/>
        </property>
        <property name="presentation_f" type="string">
            <column name="presentation_e" length ="4000" not-null="false" />
        </property>
        </class>
</hibernate-mapping>